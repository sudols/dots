# Get editor completions based on the config schema
"$schema" = 'https://starship.rs/config-schema.json'

format = """
$username$directory$git_branch$custom
$character"""

# Inserts a blank line between shell prompts
add_newline = false

[package]
disabled = false

[username]
show_always = true
style_user = "green bold"
style_root = "red bold"
format = "[$user: ]($style)"
disabled = false

[character]
success_symbol = "[â¯](bold blue)"
error_symbol = "[â¯](bold red)"
# vi stuff
vicmd_symbol = "[â®](bold yellow)"
vimcmd_visual_symbol = "[â®](bold purple)"
vimcmd_replace_symbol = "[â®](bold purple)"
vimcmd_replace_one_symbol = "[â®](bold purple)"


[directory]
read_only = "[R]"
read_only_style = "bold red"
truncation_length = 0
format = "[$path]($style)> "

[shell]
bash_indicator = "bsh"
zsh_indicator = "zsh"
fish_indicator = ''
unknown_indicator = "mystery shell"
style = "cyan bold"
disabled = true

[git_branch]
format = "[$symbol$branch(:$remote_branch)]($style) "
disabled = false

# When ranger shell is used. With multiple levels, if one ranger shell is open inside another.
[custom.ranger]
command = """
if test $RANGER_LEVEL -eq 1
    echo 'î«¬ ranger'
else if test $RANGER_LEVEL -gt 1
    echo 'î«¬ ranger~'$RANGER_LEVEL
end
"""
when = "test $RANGER_LEVEL -ge 1"
#format = "[$output]($style)"
#format = "ðŸŽ¯ [$symbol]($style) "
style = "bold green"

